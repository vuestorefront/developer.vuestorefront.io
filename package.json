{
  "private": true,
  "name": "developer.vuestorefront.io",
  "contributors": [
    {
      "email": "heitor.ramon@gmail.com",
      "name": "Heitor Ramon Ribeiro",
      "url": "https://heitor.co"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore --fix .",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "posttest": "npm run lint",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "release": "npx git-ensure -a && npx bumpp --commit --tag --push",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@iconify/vue": "^3.2.1",
    "@nuxt/content": "^2.0.0",
    "@nuxt/devalue": "^2.0.0",
    "@nuxtjs/color-mode": "^3.0.3",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@vueuse/nuxt": "^8.5.0",
    "@windicss/plugin-question-mark": "^0.1.1",
    "lodash": "^4.17.21",
    "nuxt": "^3.0.0-rc.3",
    "nuxt-windicss": "^2.3.2",
    "ohmyfetch": "^0.4.18",
    "puppeteer": "^14.1.1",
    "regexparam": "^2.0.0",
    "ufo": "^0.8.4",
    "vue": "^3.2.36",
    "windicss": "^3.5.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/prettier": "^2.6.1",
    "@types/semver": "^7.3.9",
    "@vue-storefront/eslint-config-nuxt": "^1.0.1",
    "@vue/test-utils": "^2.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

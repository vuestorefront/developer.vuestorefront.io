{
  "name": "developer.vuestorefront.io",
  "contributors": [
    {
      "email": "heitor.ramon@gmail.com",
      "name": "Heitor Ramon Ribeiro",
      "url": "https://heitor.co"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "nuxi build",
    "build:analyze": "NUXT_ANALYZE=true nuxi build",
    "dev": "nuxi dev",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .eslintignore --fix",
    "lint:content": "markdownlint ./content && case-police 'content**/*.md'",
    "lint:content:fix": "markdownlint ./content --fix && case-police 'content**/*.md' --fix",
    "lint:fix": "eslint . --ext .js,.ts,.vue --ignore-path .eslintignore  --fix",
    "posttest": "npm run lint",
    "precommit": "lint-staged",
    "prepare": "node ./scripts/prepare.js",
    "prepublishOnly": "npm run build",
    "release": "npx git-ensure -a && npx bumpp --commit --tag --push",
    "start": "node .output/server/index.mjs",
    "story:build": "histoire build",
    "story:dev": "histoire dev",
    "story:preview": "histoire preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.7",
    "@iconify/vue": "^3.2.1",
    "@nuxtjs/algolia": "^1.1.0",
    "@pinia/nuxt": "^0.3.1",
    "@popperjs/core": "^2.11.5",
    "@sendgrid/mail": "^7.7.0",
    "@supabase/supabase-js": "^1.35.6",
    "@vueuse/nuxt": "^9.0.2",
    "clipboard": "^2.0.11",
    "ejs": "^3.1.8",
    "joi": "^17.6.0",
    "nuxt": "^3.0.0-rc.8",
    "ohmyfetch": "^0.4.18",
    "pinia": "^2.0.17",
    "regexparam": "^2.0.1",
    "transform-object-keys": "^1.0.7",
    "ufo": "^0.8.5",
    "vue-github-button": "^3.0.3",
    "vue-i18n": "^9.2.0"
  },
  "devDependencies": {
    "@histoire/plugin-nuxt": "^0.10.4",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/nuxt3": "^0.2.3",
    "@nuxt/content": "^2.0.1",
    "@nuxt/test-utils": "^3.0.0-rc.6",
    "@nuxtjs/partytown": "^1.2.1",
    "@nuxtjs/tailwindcss": "^5.3.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/ejs": "^3.1.1",
    "@types/prettier": "^2.6.4",
    "@unlighthouse/nuxt": "^0.3.29",
    "@vitest/ui": "^0.20.3",
    "@vue-storefront/eslint-config": "^1.0.17",
    "@vue-storefront/nuxt-gtag": "^1.0.4",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.21.0",
    "histoire": "^0.9.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nuxt-schema-org": "^0.6.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.7.4",
    "vitest": "^0.20.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}

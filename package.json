{
  "name": "developer.vuestorefront.io",
  "contributors": [
    {
      "email": "heitor.ramon@gmail.com",
      "name": "Heitor Ramon Ribeiro",
      "url": "https://heitor.co"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "nuxi build",
    "build:analyze": "NUXT_ANALYZE=true nuxi build",
    "dev": "nuxi dev",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .eslintignore --fix",
    "lint:content": "markdownlint ./content && case-police 'content**/*.md'",
    "lint:content:fix": "markdownlint ./content --fix && case-police 'content**/*.md' --fix",
    "lint:fix": "eslint . --ext .js,.ts,.vue --ignore-path .eslintignore  --fix",
    "posttest": "npm run lint",
    "precommit": "lint-staged",
    "prepare": "node ./scripts/prepare.js",
    "prepublishOnly": "npm run build",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.3",
    "@iconify/vue": "^4.0.0",
    "@kyvg/vue3-notification": "^2.4.1",
    "@pinia/nuxt": "^0.4.2",
    "@popperjs/core": "^2.11.6",
    "@sendgrid/mail": "^7.7.0",
    "@supabase/supabase-js": "^1.35.7",
    "@vueuse/nuxt": "^9.3.0",
    "clipboard": "^2.0.11",
    "ejs": "^3.1.8",
    "joi": "^17.6.2",
    "nuxt-icon": "^0.1.6",
    "ohmyfetch": "^0.4.19",
    "pinia": "^2.0.22",
    "regexparam": "^2.0.1",
    "svg-to-pdfkit": "^0.1.8",
    "transform-object-keys": "^1.0.7",
    "ufo": "^0.8.5",
    "vue-github-button": "^3.1.0",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/nuxt3": "^0.2.4",
    "@nuxt/content": "^2.1.1",
    "@nuxt/test-utils": "3.0.0-rc.11",
    "@nuxtjs/tailwindcss": "^5.3.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/ejs": "^3.1.1",
    "@types/pdfkit": "^0.12.6",
    "@types/prettier": "^2.7.1",
    "@vitest/ui": "^0.23.4",
    "@vue-storefront/eslint-config": "^1.1.2",
    "@vue-storefront/eslint-config-typescript": "^1.1.2",
    "@vue-storefront/nuxt-gtag": "^1.0.6",
    "@vue/test-utils": "^2.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.0.0-rc.11",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
